// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}


model cards {
  Id Int @id @default(autoincrement())
  cardHolder String 
  cardNumber String
  expDate String
  cvv String
}

